{
  "name": "@laduke/zerotier-central-cli",
  "description": "Zerotier Central CLI",
  "version": "0.0.6",
  "author": "Travis LaDuke @laduke",
  "bin": {
    "ztc": "./bin/run"
  },
  "bugs": "https://github.com/laduke/zerotier-central-cli/issues",
  "dependencies": {
    "@laduke/zerotier-central-client": "0.0.3",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-autocomplete": "^0.1.5",
    "@oclif/plugin-help": "^2.2.3",
    "cli-ux": "^5.4.4",
    "conf": "^6.2.0",
    "is-cidr": "^3.1.0",
    "is-ip": "^3.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "tape": "^5.0.0-next.4"
  },
  "engines": {
    "node": ">=8.3.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/laduke/zerotier-central-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "topics": {
      "conf": { "description": "manage ztc cli config/preferences"},
      "network": { "description": "managed zerotier networks"},
      "member": { "description": "managed zerotier network members"}
    },
    "bin": "ztc",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ]
  },
  "repository": "laduke/zerotier-central-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
